---
layout: none
permalink: /feed/atom.atom
---

<?xml version="1.0" encoding="utf-8"?>
<feed xml:lang="en" xmlns="http://www.w3.org/2005/Atom">
    <id>{{ site.url }}/</id>
    <link type="text/html" rel="alternate" href="{{ site.url }}/" />
    <link rel="self" href="{{ site.url }}/feed/atom.atom" />
    <title>{{ site.name }}</title>
    <subtitle>{{ site.subtitle }}</subtitle>
    <updated>{{ site.time | date_to_xmlschema }}</updated>
    <author>
        <name>{{ site.author }}</name>
        <email>svperstarz@gmail.com</email>
    </author>
    <rights type="html">
        &amp;copy; 2013 {{ site.author }}
    </rights>
    <!--
    <category term="" scheme="" label="" />
    <contributor>
        <name>John Appleseed</name>
    </contributor>
    <generator uri="" version="">
         Example Toolkit
    </generator>
    <icon>/icon.png</icon>
    <logo>/logo.png</logo>
    -->
{% for post in site.posts %}
    <entry>
        <id>{{ site.url }}{{ post.url }}</id>
        <link type="text/html" rel="alternate" href="{{ site.url }}{{ post.url }}" />
        <title>{{ post.title | xml_escape }}</title>
        <updated>{{ post.date | date_to_xmlschema }}</updated>
        <published>{{ post.date | date_to_xmlschema }}</published>
{% for cat in post.categories %}
        <category term="{{ cat | xml_escape }}" />
{% endfor %}
{% for tag in post.tags %}
        <category term="{{ tag | xml_escape }}" />
{% endfor %}
        <summary type="html">
        <![CDATA[
{{ post.excerpt }}
        ]]>
        </summary>
        <content type="html">
        <![CDATA[
{{ post.content }}
        ]]>
        </content>
        <!--
        <contributor>
            <name>John Appleseed</name>
        </contributor>
        -->
        <!--
        <source>
            <id>http://example.org/</id>
            <title>Example</title>
            <updated>2003-12-13T18:30:02Z</updated>
            <rights>Â© 2005 Example, Inc.</rights>
        </source>
        <rights type="html">
            &amp;copy; 2013 {{ site.author }}
        </rights>
        -->
    </entry>
{% endfor %}
</feed>
